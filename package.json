{
  "name": "kaust.biostatistics.rits",
  "version": "0.9.1",
  "description": "Robust Interrupted Time Series Toolbox - KAUST Biostatistics Group",
  "main": "./index.js",
  "scripts": {
    "start": "electron index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "KAUST.Biostatistics",
  "license": "MIT/GPL",
  "devDependencies": {
    "cssnano": "^4.1.10"
  },
  "build": {
    "icon": "resources/images/app/app",
    "appId": "kaust.biostatistics.rits",
    "electronVersion": "4.1.1",
    "files": [
      {
        "from": "./",
        "to": "./"
      },
      {
        "from": "./",
        "to": "./",
        "filter": [
          "**/*.vue",
          "**/*.html"
        ]
      }
    ],
    "extraResources": [
      {
        "from": "../model/",
        "to": "./model/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraFiles": [
      {
        "from": "../model/",
        "to": "./model/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dir"
    },
    "win": {
      "target": "portable"
    }
  },
  "postinstall": "electron-builder install-app-deps",
  "dependencies": {}
}
